namespace :greeting do
desc 'outputs hello to the terminal'
task :hello do
  puts "hello from Rake!"
end
end 
## type - rake hello - to output the string "hello from Marisa!"

desc 'outputs hello to the terminal'
task :hola do
  puts "hola de Rake!"
end 
end
## type - rake hola - to output the string "hola de Rake!"